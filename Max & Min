// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int arr[10];
    printf("Enter the 10 elements if array\n");
    int i;
    for(i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
    }
    int max=arr[0];
    int min=arr[0];
    for(i=1;i<10;i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
        }
        if(min>arr[i])
        {
            min=arr[i];
        }
    }
    printf("The maximum value of array is %d\n",max);
    printf("The minimum value of array is %d",min);
    return 0;
}
